@model List<AppUser>
@{
    ViewData["Title"] = "UserList";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h1>Kullanıcı Listesi</h1>
<table class="table table-bordered">
    <tr>

        <th>AD </th>
        <th>Rol ATA</th>

    </tr>
@foreach (var item in Model)
    {
        <tr>
            <td>@item.UserName</td>
            <td><a href="/Admin/Role/AssignRole/@item.Id"class="btn btn-outline-warning">Rol Atama</a></td>
        </tr>
    }

</table>
